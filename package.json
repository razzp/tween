{
  "name": "@razzp/tween",
  "author": "Robert Wells",
  "version": "0.0.0",
  "description": "A tiny and efficient utility to tween between two values",
  "keywords": [
    "javascript",
    "typescript",
    "helper",
    "utility",
    "number",
    "value",
    "tween",
    "tweener"
  ],
  "license": "MIT",
  "bugs": "https://github.com/razzp/tween/issues",
  "homepage": "https://github.com/razzp/tween#readme",
  "repository": "github:razzp/tween",
  "type": "module",
  "main": "./dist/main.cjs",
  "exports": {
    "import": "./dist/main.js",
    "require": "./dist/main.cjs"
  },
  "types": "./types/main.d.ts",
  "files": [
    "dist/**/*.?(c)js",
    "types/**/*.d.ts"
  ],
  "scripts": {
    "prebuild": "npm run lint && npm run prettier",
    "build": "rimraf dist && rollup --config && npm run types",
    "lint": "eslint --fix *.js src/** tests/**",
    "prettier": "prettier --list-different --write *.js src/** tests/**",
    "types": "rimraf types && tsc --project tsconfig.types.json",
    "test": "jest --verbose --coverage",
    "prepublishOnly": "npm run test"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/cloudflare-turnstile": "^0.1.0",
    "@types/google.maps": "^3.50.1",
    "@types/grecaptcha": "^3.0.4",
    "@types/jest": "^29.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/youtube": "^0.0.47",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.79.0",
    "ts-jest": "^29.0.1",
    "typescript": "^4.7.4"
  }
}